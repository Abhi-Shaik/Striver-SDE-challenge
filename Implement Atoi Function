#include <bits/stdc++.h> 
int atoi(string str) {
    // Write your code here.
    int ans=0;
    int n=str.size();
    for(int i=0;i<n;i++){
        if(str[i]>='0' && str[i]<='9'){
            int a=str[i]-'0';
            ans=ans*10+a;
        }
    }
    if(str[0]=='-')
        ans=ans*-1;
    return ans;
}
